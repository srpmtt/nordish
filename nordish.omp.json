{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "template": "{{ if .WSL }}WSL at {{ end }}{{.Icon}}",
          "foreground": "#5E81AC",
          "style": "plain",
          "properties": {
            "cache_duration": "none",
            "macos": "\ue711"
          }
        },
        {
          "type": "path",
          "template": " [{{ .Path }}]",
          "foreground": "#5E81AC",
          "style": "plain",
          "properties": {
            "cache_duration": "none",
            "style": "full"
          }
        },
        {
          "type": "angular",
          "template": " \ue753 {{.Full}}",
          "foreground": "#BF616A",
          "style": "powerline",
          "properties": {
            "cache_duration": "none"
          }
        },
        {
          "type": "dotnet",
          "template": " \ue77f {{ .Full }}",
          "foreground": "#B48EAD",
          "style": "powerline",
          "properties": {
            "cache_duration": "none"
          }
        },
        {
          "type": "elixir",
          "template": " \ue62d {{ .Full }} ",
          "foreground": "#B48EAD",
          "style": "powerline",
          "properties": {
            "cache_duration": "none"
          }
        },
        {
          "type": "go",
          "template": " \ufcd1 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#88C0D0",
          "style": "powerline",
          "properties": {
            "cache_duration": "none"
          }
        },
        {
          "type": "haskell",
          "template": " \ue61f {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#B48EAD",
          "style": "powerline",
          "properties": {
            "cache_duration": "none"
          }
        },
        {
          "type": "java",
          "template": " \ue738 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#D08770",
          "style": "powerline",
          "properties": {
            "cache_duration": "none"
          }
        },
        {
          "type": "lua",
          "template": " \ue620 {{ .Full }}",
          "foreground": "#B48EAD",
          "style": "powerline",
          "properties": {
            "style": "powerline",
            "cache_duration": "none"
          }
        },
        {
          "type": "node",
          "template": " \ue718 {{ if .PackageManagerIcon }}{{ .PackageManagerIcon }} {{ end }}{{ .Full }}",
          "foreground": "#A3BE8C",
          "style": "powerline",
          "properties": {
            "cache_duration": "none"
          }
        },
        {
          "type": "ocaml",
          "template": " \ue67a {{ .Full }}",
          "foreground": "#D08770",
          "style": "powerline",
          "properties": {
            "cache_duration": "none"
          }
        },
        {
          "type": "python",
          "template": " \ue235 {{ if .Error }}{{ .Error }}{{ else }}{{ if .Venv }}{{ .Venv }} {{ end }}{{ .Full }}{{ end }}",
          "foreground": "#EBCB8B",
          "style": "powerline",
          "properties": {
            "cache_duration": "none"
          }
        },
        {
          "type": "react",
          "template": " \ue7ba {{.Full}}",
          "foreground": "#81A1C1",
          "style": "powerline",
          "properties": {
            "cache_duration": "none"
          }
        },
        {
          "type": "ruby",
          "template": " \ue791 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }}",
          "foreground": "#BF616A",
          "style": "powerline",
          "properties": {
            "cache_duration": "none"
          }
        },
        {
          "type": "rust",
          "template": " \ue7a8 {{ .Full }}",
          "foreground": "#BF616A",
          "style": "powerline",
          "properties": {
            "cache_duration": "none"
          }
        },
        {
          "type": "zig",
          "template": " îš© {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#ffad55",
          "style": "powerline"
        },
        {
          "type": "git",
          "template": " {{ .HEAD }}{{ if gt .StashCount 0 }} \uf692 {{ .StashCount }}{{ end }} ",
          "foreground": "#A3BE8C",
          "style": "plain",
          "properties": {
            "branch_icon": "\ueafc",
            "cache_duration": "none",
            "fetch_stash_count": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "julia",
          "template": " \ue624 {{ if .Error }}{{ .Error }}{{ else }}{{ .Full }}{{ end }} ",
          "foreground": "#5E81AC",
          "style": "powerline",
          "properties": {
            "cache_duration": "none"
          }
        },
        {
          "type": "text",
          "template": "\u2514\u2500 ",
          "foreground": "#5E81AC",
          "style": "plain",
          "properties": {
            "cache_duration": "none"
          }
        }
      ],
      "newline": true
    }
  ],
  "version": 3
}
